#!/bin/bash

if [ ! -e ${HOME}/.cargo ]; then
    curl https://sh.rustup.rs -sSf | sh
    rustup install nightly
    rustup default nightly
    rustup completions zsh > ~/.zfunc/_rustup
fi

brew install go

if [ ! -e ${HOME}/.pyenv ]; then
    git clone https://github.com/yyuu/pyenv.git ~/.pyenv
fi
